import java.util.Scanner;

public class main {
	public static void main(String args[]) {
		Scanner scanner = new Scanner(System.in);
		String input = "";
		while (scanner.hasNextInt()) {
			int run = scanner.nextInt();
			if (input.equals("0"))
				break;

			for (int i = 0; i < input.length(); i++) {
				input.charAt(i) = run;
			}
		}
	}
}