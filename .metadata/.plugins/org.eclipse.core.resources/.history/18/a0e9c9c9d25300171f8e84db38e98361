import java.util.Scanner;

public class main {
	private static int temp;

	public static void main(String args[]) {
		
		Scanner scanner = new Scanner(System.in);
		while (true) {//一直連續讀進來,計算,輸出
			int run = scanner.nextInt();
			int mod = scanner.nextInt();

			if (run == 0 && mod == 0) {
				System.out.println("0" + " " + "0");
				break;
			}
			
			int[] arrrun = new int[run];//array初始化
			for (int i = 0; i < run; i++) {
				arrrun[i] = scanner.nextInt();//陣列讀入之後存到要放的陣列arrrun裡面
			}

			for (int i = 0; i < run; i++) {
				for (int j = i + 1; j < run; j++) {
					int a = arrrun[i] % mod;
					int b = arrrun[j] % mod;

					if (a > b) {
						int temp = arrrun[i];
						arrrun[i] = arrrun[j];
						arrrun[j] = temp;
					} else if (a == b) {
						if ((arrrun[i] % 2 == 1) && (arrrun[j] % 2 == 1) && (arrrun[i] < arrrun[j])) {
							int temp = arrrun[i];
							arrrun[i] = arrrun[j];
							arrrun[j] = temp;
						} else if (arrrun[i] % 2 == 0 && arrrun[j] % 2 == 1) {
							int temp = arrrun[i];
							arrrun[i] = arrrun[j];
							arrrun[j] = temp;
						}
						else if (arrrun[i] % 2 == 0 && arrrun[j] % 2 == 0 && (arrrun[i] > arrrun[j])) {
							int temp = arrrun[i];
							arrrun[i] = arrrun[j];
							arrrun[j] = temp;
						}

					}
				}
			}

			for (int i = 0; i < run; i++) {
				System.out.println(arrrun[i]);
			}
		}
	}
}
