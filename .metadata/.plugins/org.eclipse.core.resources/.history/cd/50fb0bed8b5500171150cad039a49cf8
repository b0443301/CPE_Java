import java.util.Scanner;

public class main {
	public static void main(String args[]) {
		int count = 0;
		Scanner scanner = new Scanner(System.in);
		Boolean Flag = true;
		 
		while (scanner.hasNextLine()) {
			String input = scanner.nextLine();
			// if (input.equals("0")) {
			// break;
			// }
			for (int i = 0; i < input.length(); i++) {
				if (input.charAt(i) == '\"') {
//					count++;
//					if (count % 2 == 1) {
//						System.out.print("``");
//					} else {
//						System.out.print("''");
//					}
					
					Flag = !Flag;
					if (Flag) {
						System.out.print("``");
					} else {
						System.out.print("''");
					}
				} else {
					System.out.print(input.charAt(i));
				}
			}
			System.out.print("\n");
		}
	}
}

