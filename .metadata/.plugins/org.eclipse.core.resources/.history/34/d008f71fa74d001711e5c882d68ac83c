import java.util.Arrays;
import java.util.Scanner;
public class main {
	public static void main(String arg[]){
		  //init
			int input=0,ans=0;
			 for (int i = 1; i <499; i++) { 
		            input = i;
		        }
			int[] x = null;
			
			x = new int[input];
			 for (int i = 1; i < 2999; i++) { 
		            x[input] = i;
		        }
			Scanner scanner = new Scanner(System.in);
			//read
			input = scanner.nextInt();
			x[input]= scanner.nextInt();
			//calc
			Arrays.sort(x);
			for(int i=0;i<=x[input];i++){
				if(i %2 ==1){
					System.out.println("_计い旒="+i);
				}else{
			 System.out.println("案计い旒="+i);
				}
			

			}
			}
	}
