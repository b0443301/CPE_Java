import java.util.Scanner;

public class main {
	public static void main(String args[]) {
		Scanner scanner = new Scanner(System.in);
		while (scanner.hasNextLine()) {
			String run1 = scanner.nextLine();
			String run2 = scanner.nextLine();
			String[] run2Split = run2.split(" ");
			int sum=0;
			int run1Int=Integer.parseInt(run1);
			int run2Int=Integer.parseInt(String.valueOf(run2Split));
			for(int i=0;i<run2Split.length-1;i++){
				 sum+=run2Split*(Math.pow(run1Int, i));
			}
			
		}
	}
}
