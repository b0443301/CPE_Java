import java.util.Scanner;

public class main {
	private static int temp;

	public static void main(String args[]) {
		
		Scanner scanner = new Scanner(System.in);
		while (true) {//@匠s尿弄iㄓ,p衡,块X
			int run = scanner.nextInt();
			int mod = scanner.nextInt();

			if (run == 0 && mod == 0) {
				System.out.println("0" + " " + "0");
				break;
			}
			
			int[] arrrun = new int[run];//array飑lて
			for (int i = 0; i < run; i++) {
				arrrun[i] = scanner.nextInt();//}C弄Jぇ幡s飙n癃喊}Carrrun柑
			}

			for (int i = 0; i < run; i++) {
				for (int j = i + 1; j < run; j++) {
					int a = arrrun[i] % mod;//衡Xe奔痞r壕l计
					int b = arrrun[j] % mod;//衡X岘奔痞r壕l计

					if (a > b) {//pGe奔痞r壕l计>岘奔痞r壕l计
						int temp = arrrun[i];//h暗ユ传,TOe奔痞r壕l计<岘奔痞r壕l计
						arrrun[i] = arrrun[j];
						arrrun[j] = temp;
					} else if (a == b) {
						if ((arrrun[i] % 2 == 1) && (arrrun[j] % 2 == 1) && (arrrun[i] < arrrun[j])) {
							int temp = arrrun[i];
							arrrun[i] = arrrun[j];
							arrrun[j] = temp;
						} else if (arrrun[i] % 2 == 0 && arrrun[j] % 2 == 1) {
							int temp = arrrun[i];
							arrrun[i] = arrrun[j];
							arrrun[j] = temp;
						}
						else if (arrrun[i] % 2 == 0 && arrrun[j] % 2 == 0 && (arrrun[i] > arrrun[j])) {
							int temp = arrrun[i];
							arrrun[i] = arrrun[j];
							arrrun[j] = temp;
						}

					}
				}
			}

			for (int i = 0; i < run; i++) {
				System.out.println(arrrun[i]);
			}
		}
	}
}
