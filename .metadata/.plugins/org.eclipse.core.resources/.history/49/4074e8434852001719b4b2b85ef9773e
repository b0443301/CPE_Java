import java.util.Scanner;

public class main {
	public static void main() {
		int num1 = 0, num2 = 0, ans1 = 0, ans2 = 0;
		Scanner scanner = new Scanner(System.in);
		// read
		num1 = scanner.nextInt();
		num2 = scanner.nextInt();
		// cal
		while (true) {
			if (num1 % 2 == 1)
				ans1 = (num1 * 3) + 1;
			else if (num1 % 2 == 0)
				ans1 = (num1) / 2;
		}
		while (true) {
			if (num2 % 2 == 1)
				ans2 = (num2 * 3) + 1;
			else if (num2 % 2 == 0)
				ans2 = (num2) / 2;
		}

	}
}
