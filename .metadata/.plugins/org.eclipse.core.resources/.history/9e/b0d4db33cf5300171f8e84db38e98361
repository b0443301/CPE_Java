import java.util.Scanner;

public class main {
	private static int temp;

	public static void main(String args[]) {
		// init
		int run = 0, mod = 0;
		int[] arrrun;
		// read
		Scanner scanner = new Scanner(System.in);
		run = scanner.nextInt();
		mod = scanner.nextInt();
		arrrun = new int[run];
		if (run == 0 && mod == 0) {
			System.out.println("0" + " " + "0");
		}
		for (int i = 0; i < run; i++) {
			arrrun[i] = scanner.nextInt();

		}
		for (int i = 0; i < run; i++) {
			for (int j = i + 1; j < run; j++) {
				int odd = arrrun[i] % mod;
				int even = arrrun[j] % mod;

				if (odd > even) {
					int temp = arrrun[i];
					arrrun[i] = arrrun[j];
					arrrun[j]= temp;

				} else if (odd == even) {
					if (arrrun[i] % 2 == 1 && arrrun[j] % 2 == 1 && (arrrun[i] < arrrun[j])) {
						int temp = arrrun[i];
						arrrun[i] = arrrun[j];
						arrrun[j]= temp;;
					}
					if (arrrun[i] % 2 == 0 && arrrun[j] % 2 == 1) {
						int temp = arrrun[i];
						arrrun[i] = arrrun[j];
						arrrun[j]= temp;
					}
					if (arrrun[i] % 2 == 1 && arrrun[j] % 2 == 0 && (arrrun[i] < arrrun[j])) {
						int temp = arrrun[i];
						arrrun[i] = arrrun[j];
						arrrun[j]= temp;
					}
					if (arrrun[i] % 2 == 0 && arrrun[j] % 2 == 0) {
						int temp = arrrun[i];
						arrrun[i] = arrrun[j];
						arrrun[j]= temp;
					}
					
				}
				System.out.println(arrrun[j]);

			}
		}
		




	}
}
